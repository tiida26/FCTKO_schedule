<script type="text/javascript">
$(function(){
	$('.nav').hover(function(){
		var nav = $(this).find('li').length;
		$(this).animate({'height':28 * nav + 'px'},'fast');
	},function(){
		$(this).animate({'height':'26px'},'fast');
	});
});
</script>

